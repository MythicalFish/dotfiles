{
  "editor.tabSize": 2,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true,
    "**/log": true
  },
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "explorer.openEditors.visible": 0,
  "editor.fontSize": 14,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.iconTheme": "vscode-great-icons",
  "window.zoomLevel": 0,
  "window.newWindowDimensions": "inherit",
  // VIM
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#000",
  "vim.statusBarColors.insert": "#ff0054",
  "vim.statusBarColors.visual": "#00ff1e",
  "vim.statusBarColors.visualline": "#00ff1e",
  "vim.statusBarColors.visualblock": "#00ff1e",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["s"],
      "after": ["leader", "leader", "s"]
    }
  ],
  "vim.ignorecase": false,
  // "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.sneak": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true,
  "vim.disableExtension": false,
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.rb"
  ],
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.keymap": "none",
  "explorer.confirmDelete": false,
  "editor.minimap.enabled": true,
  "editor.renderControlCharacters": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": true,
  "go.lintOnSave": "file",
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "flow.pathToFlow": "${workspaceRoot}/node_modules/.bin/flow",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "advancedNewFile.exclude": {
    "node_modules": true
  },
  "randomThemeSwitcher.themeList": [
    "Monokai",
    "Popping and Locking",
    "Horizon Bold",
    "Deepdark Material Theme | Full Black Version",
    "Panda Syntax",
    "Winter is Coming (Dark Blue)",
    "An Old Hope",
    "Eva Dark Bold",
    "Cobalt2",
    "Hyper Term Black"
  ],
  "workbench.colorCustomizations": {
    "statusBar.background": "#000",
    "statusBar.noFolderBackground": "#000",
    "statusBar.debuggingBackground": "#000"
  },
  "indentRainbow.colors": [
    "rgba(255,0,255,0.15)",
    "rgba(0,255,255,0.15)",
    "rgba(255,255,0,0.15)",
    "rgba(255,0,0,0.15)"
  ],
  "workbench.colorTheme": "Hyper Term Black",
  "prettier.disableLanguages": [
    "html"
  ],
  "go.useLanguageServer": true
}
